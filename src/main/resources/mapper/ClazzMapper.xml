<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.apps.sms.dao.ClazzDao">
  <select id="selectAll" resultType="com.briup.apps.sms.bean.Clazz">
    select * from sms_clazz
  </select>
   <insert id="">
   insert into sms_clazz values(null,#{name},#{begin_time})
   </insert>
   
   
 <update id=""> update sms_clazz
 set id=#{id}
  id=#{clazz_id}
  where id=#{id}</update>


</mapper>